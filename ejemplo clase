package ejercicio6A;

public class Atributos {
    String marca="Ford",modelo="2014",color="Amarillo";
    double precio=178000.345555;
    public static void main(String [] args){
        //Crear una instancia de mi clase,  es decir un objeto
        Atributos instancia1 = new Atributos();
        instancia1.marca = "Chevrolet";
        //Accediendo a los atributos de la instancia
        System.out.println("Marca: " + instancia1.marca);
        System.out.println("Modelo: " + instancia1.modelo);
        System.out.println("Color: " + instancia1.color);
        System.out.println("Precio: " + instancia1.precio);
        
    }
  
}
